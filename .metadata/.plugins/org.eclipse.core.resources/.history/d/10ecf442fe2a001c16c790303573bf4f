package proxy;

public class MyAspect implements Developer{
	
	private Developer developer;
	
	public MyAspect(String className) {
		
		switch (className) {
		case "proxy.Man":
			developer = new Man();
			break;
		case "proxy.Woman":
			developer = new Woman();
			break;
		default: throw new RuntimeException("잘못된 클래스명을 넣었습니다.");
		}
	}
	
	@Override
	public void develop() {
		// TODO Auto-generated method stub
		
	}
}
