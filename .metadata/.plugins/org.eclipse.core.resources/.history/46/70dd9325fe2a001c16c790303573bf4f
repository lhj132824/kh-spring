package proxy;

public class MyAspect implements Developer{
	
	private Developer developer;
	
	public MyAspect(String className) {
		
		switch (className) {
		case "proxy.Man":
			developer = new Man();
			break;
		case "proxy.Woman":
			developer = new Woman();
			break;
		default:
			break;
		}
	}
	
	@Override
	public void develop() {
		// TODO Auto-generated method stub
		
	}
}
